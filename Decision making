#include<iostream>
using namespace std;
int main()
{
    int age;
    cout<<"PLEASE ENTER YOUR AGE : ";
    cin>>age;
    if (age>17)
        cout<<"YOU ARE ELIGIBLE TO VOTE";
    else {
        if (age == 17)
            cout << "YOU WILL BE ELIGIBLE NEXT YEAR ";
        else
            cout << "YOU ARE NOT ELIGIBLE";

    }
    return 0;

}
