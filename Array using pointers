#include<iostream>
using namespace std;
int main()
{    int *p;
    int elements[5];
    cout<<"Enter elements: "<<endl;
    for(int i=0;i<5;i++) {
        cin >> elements[i];
    }

    p=elements;
    cout<<"You entered: "<<endl;
    for(int o=0;o<5;o++)
    {cout<<*p<<endl;
    p++;}
    return 0;
}
